  <methodlist><method name="reset">
      <arglist />
      <digest>
        Return the object to its original state
      </digest>
      <description>
        Calling <m>reset</m> will change parameter values back to those the object was <i>created</i> with, i.e. any attribute values set in the box will be retained
      </description>
    </method>
    <method name='fittransform'>
			<arglist><arg name= "sourceDataSet" optional='0' type="symbol" /><arg name= "destDataSet" optional='0' type="symbol" /></arglist>
			<description>
        <div class="document">
<p>Fit the model to a <o>fluid.dataset~</o> and write the new projected data to a destination FluidDataSet.</p>
</div>

        <h5>Argument details:</h5>
        <ul><li>sourceDataSet: <div class="document">
<p>Source data, or the DataSet name</p>
</div>
</li><li>destDataSet: <div class="document">
<p>Destination data, or the DataSet name</p>
</div>
</li></ul>
			</description>
		</method>
    </methodlist>
	<!--ATTRIBUTES-->
	<attributelist><attribute name='distancemetric' get='1' set='1' type='int' size='1'>
        <digest>
          Distance Metric
        </digest>
        <description>
          <div class="document">
<p>The distance metric to use (integer, 0-6, see flags above)</p>
</div>

          
        </description><attributelist><attribute name="default" get="1" set="1" type="int" size='1' value='1' /></attributelist></attribute><attribute name='numdimensions' get='1' set='1' type='int' size='1'>
